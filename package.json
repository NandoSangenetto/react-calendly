{
  "name": "react-calendly",
  "version": "1.3.0",
  "description": "Calendly integration for React apps",
  "author": "tcampb",
  "license": "MIT",
  "repository": "tcampb/react-calendly",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "react",
    "calendly",
    "component"
  ],
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -p tsconfig.production.json --emitDeclarationOnly",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "npm install && npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 9001 -c .storybook ",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/cli": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.28",
    "@svgr/rollup": "^5.4.0",
    "@testing-library/react": "^11.1.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "faker": "^5.1.0",
    "gh-pages": "^3.1.0",
    "mini-css-extract-plugin": "^1.2.1",
    "prettier": "2.1.2",
    "react": "^16.4.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-dom": "^16.4.1",
    "react-scripts": "4.0.0",
    "rollup": "^2.33.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5"
  },
  "files": [
    "dist",
    "typings"
  ]
}
